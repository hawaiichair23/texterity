    {
      "id": 2,
      "name": "Script 2",
      "code": "const myText = createTextObject({\n  text: \"DEOXYRIBONUCLEICACID\",\n  x: 640,\n  y: 360,\n  fontSize: 52,\n  color: 0xFF00FF\n});\n\nlet time = 0;\nsetInterval(() => {\n  const offsets = [];\n  const textLength = 20;\n  \n  for (let i = 0; i < textLength; i++) {\n    const t = (i / textLength) * Math.PI * 6 + time;\n    \n    // Double helix equations\n    const helix1Y = Math.sin(t) * 30;\n    const helix1Z = Math.cos(t) * 60;\n    \n    // Use Z for depth simulation (scale effect)\n    const x = helix1Z;\n    const y = helix1Y;\n    \n    offsets.push({ x, y });\n  }\n  \n  setCharacterOffsets(myText, offsets);\n  time += 0.08;\n}, 16);",
      "collapsed": false,
      "isRunning": true
    }
